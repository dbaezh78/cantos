{
  "cantos": [
    {
      "id": 1,
      "titulo": "A TI, SEÑOR, SE DEBE LA ALABANZA EN SIÓN",
      "subtitulo": "Salmo 65 (64)",
      "secciones": [
        {
          "tipo": "A",
          "posicion": "izquierda",
          "estrofas": [
            {
              "texto": "A ti, Señor, se debe la alabanza en Sión,\nse cumple lo que prometiste.",
              "acordes": [
                {"acorde": "Re", "posicion": 5, "base": "Re"},
                {"acorde": "Sol", "posicion": 215, "base": "Sol"}
              ]
            }
          ]
        },
        {
          "tipo": "C",
          "posicion": "derecha",
          "estrofas": [
            {
              "texto": "Tu que escucha nuestra oración,\nviene todo mortal",
              "acordes": [
                {"acorde": "La", "posicion": 10, "base": "La"},
                {"acorde": "Mi", "posicion": 35, "base": "Mi"}
              ]
            }
          ]
        },
        {
          "tipo": "A",
          "posicion": "izquierda",
          "estrofas": [
            {
              "texto": "Los pecados me dominan,\npero tú nos perdonas.",
              "acordes": [
                {"acorde": "Re", "posicion": 8, "base": "Re"},
                {"acorde": "Sol", "posicion": 40, "base": "Sol"}
              ]
            }
          ]
        }
      ],
      "info": [
        "Autor: Anónimo",
        "Tono: Salmódico",
        "Uso: Liturgia de las Horas"
      ]
    }
  ]
}